    "autoload": {
      "psr-4": {
        "Sample\\": "src/"
      }
    },
    "autoload-dev": {
      "psr-4": {
      }
    },
    "bin": [],
    "require-dev": {
        "phpunit/phpunit": "^9.5"
    }
